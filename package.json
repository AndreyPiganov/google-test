{
    "name": "google-test",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "npm run db:migrate:run && nest start",
        "start:dev": "npm run db:migrate:run && nest start --watch",
        "start:debug": "npm run db:migrate:run && nest start --debug --watch",
        "start:prod": "npm run db:migrate:run && node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "tsc": "tsc",
        "build:prod": "npm run build",
        "db:rollback": "dotenv -- node_modules/.bin/knex migrate:rollback",
        "db:migrate:make": "dotenv -- node_modules/.bin/knex migrate:make",
        "db:migrate:run": "dotenv -- node_modules/.bin/knex migrate:latest",
        "db:seed:make": "dotenv -- node_modules/.bin/knex seed:make",
        "db:seed:run": "dotenv -- node_modules/.bin/knex seed:run"
    },
    "dependencies": {
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.3.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/mapped-types": "^2.0.6",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/schedule": "^4.1.1",
        "@nestjs/swagger": "^8.0.5",
        "@types/knex": "^0.15.2",
        "axios": "^1.7.7",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "dotenv": "^16.4.5",
        "dotenv-cli": "^7.4.2",
        "googleapis": "^144.0.0",
        "knex": "^3.1.0",
        "nest-knexjs": "^0.0.24",
        "nest-winston": "^1.9.7",
        "pg": "^8.13.1",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "swagger-ui-express": "^5.0.1",
        "tslib": "^2.8.1",
        "winston": "^3.16.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/axios": "^0.9.36",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.14",
        "@types/node": "^20.3.1",
        "@types/supertest": "^6.0.0",
        "@typescript-eslint/eslint-plugin": "^7.0.0",
        "@typescript-eslint/parser": "^7.0.0",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-jsdoc": "^50.4.3",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-unused-imports": "^4.1.4",
        "jest": "^29.7.0",
        "jsdoc-tsimport-plugin": "^1.0.5",
        "prettier": "^3.3.3",
        "prettier-plugin-jsdoc": "^1.3.0",
        "prettier-plugin-sql": "^0.18.1",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
